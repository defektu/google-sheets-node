{
	"version": 2,
	"name" : "express",
	"builds": [
		{
			"src": "./index.js",
			"use": "@vercel/node"
			
		},
		{
			"src": "public/**",
			"use": "@vercel/static",
			"config": { "includeFiles": ["public/**"] }
		  }
	],
	"routes": [
		{
            "src": "/",
            "dest": "public/index.html"
        },
        {
            "src": "/(.+)",
            "dest": "public/$1"
        }
	]
  }